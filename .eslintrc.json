    "game": "readonly",
    "getDocumentClass": "readonly",
    "loadTemplates": "readonly",
    "renderTemplate": "readonly",
    "CONFIG": "writable",
    "CONST": "readonly",
    "Actor": "readonly",
    "ActorSheet": "readonly",
    "Actors": "readonly",
    "ChatMessage": "readonly",
    "Combat": "readonly",
    "CombatTracker": "readonly",
    "Combatant": "readonly",
    "Dialog": "readonly",
    "Handlebars": "readonly",
    "Hooks": "readonly",
    "Item": "readonly",
    "ItemSheet": "readonly",
    "Items": "readonly",
    "Roll": "readonly",
    "Ruler": "readonly",
    "TextEditor": "readonly"
  },
  "plugins": ["jest", "jsdoc"]
}
