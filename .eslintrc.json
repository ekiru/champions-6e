{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:jest/recommended",
    "plugin:jest/style",
    "plugin:jsdoc/recommended",
    "prettier"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {},
  "globals": {
    "foundry": "readonly",
    "CONFIG": "writable",
    "Actor": "readonly",
    "ActorSheet": "readonly",
    "Actors": "readonly",
    "Hooks": "readonly",
    "Item": "readonly",
    "TextEditor": "readonly"
  },
  "plugins": ["jest", "jsdoc"]
}
